mixin steps-graph(percent1, name1, percent2, name2, percent3, name3)
	.how-we-make__step
		.how-we-make__step-item.how-we-make__step-item-top
			.how-we-make__step-item-text
				.h1= percent1
				p.font-weight-bold!= name1
		.how-we-make__step-item.how-we-make__step-item-middle
			.how-we-make__step-item-text
				.h1= percent2
				p.font-weight-bold!= name2
		.how-we-make__step-item.how-we-make__step-item-bottom
			.how-we-make__step-item-text
				.h1= percent3
				p.font-weight-bold!= name3
