			.row.align-items-center
				.col-lg-4.col-xl-3.text-center
					.h4.mb-1= title
						span.text-white= title_white
					p= text
				.col-lg-8.col-xl-9.px-xl-5.pt-4.pt-lg-0 
					.row
						.col-md-6.pb-4.pb-md-0
