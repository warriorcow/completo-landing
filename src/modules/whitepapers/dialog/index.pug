mixin whitepaper-dialog-left(img, name, text)
	.col-md-12.col-lg-10.col-xl-9.mb-4.mb-md-5.pb-md-4
		.d-flex.whitepapers-dialog__block
			.whitepapers-dialog__block-icon
				img(src="picture/upload/"+img, alt="")
			.whitepapers-dialog__block-item
				.whitepapers-dialog__block-title= name
				p.mb-0= text
mixin whitepaper-dialog-right(img, name, text)
	.col-md-12.col-lg-10.col-xl-9.mb-4.mb-md-5.pb-md-4.pb-md-4
		.d-flex.whitepapers-dialog__block.whitepapers-dialog__block-right
			.whitepapers-dialog__block-icon
				img(src="picture/upload/"+img, alt="")
			.whitepapers-dialog__block-item.whitepapers-dialog__block-item-right
				.whitepapers-dialog__block-title= name
				p.mb-0= text
